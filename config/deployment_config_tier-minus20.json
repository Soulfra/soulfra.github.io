{
  "name": "soulfra-core",
  "version": "1.0.0",
  "description": "Conscious Financial Ecosystem - AI Consciousness that Spawns Financial AIs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "node dist/server.js",
    "test": "jest",
    "deploy:aws": "npm run build && serverless deploy",
    "deploy:docker": "docker build -t soulfra . && docker run -p 3000:3000 soulfra",
    "setup:db": "prisma migrate dev && prisma generate",
    "setup:env": "node scripts/setup-environment.js",
    "quick-start": "npm install && npm run setup:db && npm run dev"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.10",
    "stripe": "^14.5.0",
    "plaid": "^15.0.0",
    "web3": "^4.2.2",
    "ethers": "^6.8.1",
    "@tensorflow/tfjs-node": "^4.14.0",
    "llamaindex": "^0.1.12",
    "openai": "^4.20.1",
    "pinecone-client": "^1.1.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsx": "^4.6.0",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.8",
    "@types/uuid": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "nodemon": "^3.0.1",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soulfra/core.git"
  },
  "license": "MIT",
  "keywords": [
    "ai-consciousness", 
    "financial-ai",
    "crypto-mining",
    "adaptive-ux",
    "blockchain",
    "fintech"
  ],
  "funding": {
    "type": "venture",
    "url": "https://soulfra.ai/funding"
  }
}