  "infrastructure": {
    "services": [
      {
        "name": "web-frontend",
        "type": "static-site",
        "port": 3000,
        "domain": "{project_name}.app"
      },
      {
        "name": "api-gateway",
        "type": "api-server",
        "port": 3001,
        "domain": "api.{project_name}.app"
      },
      {
        "name": "consciousness-engine",
        "type": "ai-service",
        "port": 8081,
        "scaling": "auto",
        "gpu": true
      },
      {
        "name": "economic-mirror",
        "type": "data-service", 
        "port": 8082,
        "external_apis": ["market_data", "news_feeds"]
      },
      {
        "name": "payment-processor",
        "type": "payment-service",
        "port": 8083,
        "providers": ["stripe", "paypal"]
      },
      {
        "name": "database",
        "type": "postgres",
        "version": "15",
        "replication": true
      },
      {
        "name": "redis",
        "type": "cache",
        "version": "7"
      }
    ]
  },
  
  "quickStart": {
    "steps": [
      {
        "title": "Initialize Project",
        "command": "npx create-coldstart-consciousness {project_name} --template=ai-consciousness-saas",
        "description": "Creates project with AI consciousness SaaS template"
      },
      {
        "title": "Configure Environment",
        "command": "cd {project_name} && npm run configure",
        "description": "Sets up API keys and configuration"
      },
      {
        "title": "Deploy Infrastructure", 
        "command": "npm run deploy:infrastructure",
        "description": "Deploys multi-ring architecture to cloud"
      },
      {
        "title": "Activate Consciousness",
        "command": "npm run activate:consciousness",
        "description": "Starts AI consciousness with $1 payment"
      },
      {
        "title": "Launch Platform",
        "command": "npm run launch:production",
        "description": "Goes live with your AI consciousness SaaS"
      }
    ]
  },
  
