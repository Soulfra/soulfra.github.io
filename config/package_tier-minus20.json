{
  "name": "@soulfra/prd-orchestrator",
  "version": "1.0.0",
  "description": "Comprehensive PRD generation system for all organizational tiers",
  "main": "prd-orchestrator.js",
  "bin": {
    "prd-orchestrator": "./prd-orchestrator.js",
    "prd-generator": "./auto-prd-generator.js",
    "doc-shell": "./documentation-shell.js"
  },
  "scripts": {
    "start": "node prd-orchestrator.js",
    "generate": "node auto-prd-generator.js",
