{
  "plugins": {
    "wordpress": {
      "name": "MirrorOS Cal Assistant",
      "version": "1.0.0",
      "path": "wp-plugin/mirroros-cal.php",
      "download": "wp-plugin/mirroros-cal.zip",
      "description": "Embed Cal on your WordPress site with one click",
      "features": [
        "One-click installation",
        "Customizable widget position",
        "Voice input support",
        "Deep reflection mode",
        "Export capabilities",
        "Shortcode support"
      ],
      "requirements": {
        "wordpress": "5.0+",
        "php": "7.2+",
        "apiKey": "optional"
      }
    },
    "shopify": {
      "name": "Cal for Shopify",
      "status": "coming-soon",
      "description": "AI customer support for your Shopify store"
    },
    "webflow": {
      "name": "Cal Webflow Component",
      "status": "coming-soon",
      "description": "Drag-and-drop Cal into your Webflow site"
    },
    "react": {
      "name": "react-mirroros-cal",
      "status": "available",
      "npm": "@mirroros/cal-react",
      "description": "React component for Cal integration"
    },
    "vanilla": {
      "name": "MirrorOS Cal Embed",
      "status": "available",
      "cdn": "https://cdn.mirror.os/cal-embed.js",
      "description": "Pure JavaScript embed for any website"
    }
  },
  "installation": {
    "wordpress": [
      "Download mirroros-cal.zip",
      "Upload to WordPress Plugins",
      "Activate plugin",
      "Configure in Settings > MirrorOS Cal",
      "Use [cal_assistant] shortcode or widget"
    ],
    "vanilla": [
      "Add script tag to your HTML",
      "<script src='https://cdn.mirror.os/cal-embed.js'></script>",
      "Initialize with: MirrorOS.Cal.init({ apiKey: 'your-key' })"
    ]
  },
  "support": {
    "documentation": "https://docs.mirror.os/embed",
    "issues": "https://github.com/mirroros/cal-embed/issues",
    "community": "https://discord.gg/mirroros"
  }
}